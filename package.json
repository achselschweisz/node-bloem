  "dependencies": {
    "fnv": "git@github.com:achselschweisz/node-fnv.git#es6",
    "bitbuffer": "git@github.com:achselschweisz/node-bitbuffer.git#es6"
  },
  "devDependencies": {
    "changelog42": "0.9.0",
    "eslint": "2.13.1",
    "istanbul": "0.4.4",
    "mocha": "2.5.3"
  },
  "scripts": {
    "lint": "eslint ./",
    "test": "npm run lint && istanbul cover _mocha -- ./test",
